from twisted.web.woven import model, page


class SomeObjectWithAttributes(model.AttributeModel):
    one = '1'
    two = '2'
    three = '3'


model = SomeObjectWithAttributes()

resource = page.Page(model, template = """<html>
    <body>
        <div model="one" />
        <div model="two" />
        <div model="three" />
    </body>
</html>""")

